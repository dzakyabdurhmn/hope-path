ALTER TABLE debt
  ADD PRIMARY KEY (id),
  ADD KEY Debt_userId_fkey (userId);

ALTER TABLE rehabilitation
  ADD PRIMARY KEY (id);

ALTER TABLE task
  ADD PRIMARY KEY (id);

ALTER TABLE user
  ADD PRIMARY KEY (id),
  ADD UNIQUE KEY User_clerkUserId_key (clerkUserId);

ALTER TABLE usertask
  ADD PRIMARY KEY (id),
  ADD KEY DailyTask_userId_fkey (userId),
  ADD KEY Task_fk (taskId);


ALTER TABLE debt
  MODIFY id int NOT NULL AUTOINCREMENT, AUTOINCREMENT=31;

ALTER TABLE rehabilitation
  MODIFY id int NOT NULL AUTOINCREMENT, AUTOINCREMENT=136;

ALTER TABLE task
  MODIFY id int NOT NULL AUTOINCREMENT, AUTOINCREMENT=5;

ALTER TABLE user
  MODIFY id int NOT NULL AUTOINCREMENT, AUTOINCREMENT=5;

ALTER TABLE usertask
  MODIFY id int NOT NULL AUTOINCREMENT, AUTOINCREMENT=21;


ALTER TABLE debt
  ADD CONSTRAINT Debt_userId_fkey FOREIGN KEY (userId) REFERENCES user (id) ON DELETE RESTRICT ON UPDATE CASCADE;

ALTER TABLE usertask
  ADD CONSTRAINT DailyTask_userId_fkey FOREIGN KEY (userId) REFERENCES user (id) ON UPDATE RESTRICT,
  ADD CONSTRAINT Task_fk FOREIGN KEY (taskId) REFERENCES task (id) ON UPDATE RESTRICT;




